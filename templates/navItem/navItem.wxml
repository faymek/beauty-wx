<template name="navItem">
  <view class="navItems" bindtap="toCate" data-id="id">
    <view wx:for="{{navItems}}" wx:key="key" style='width:{{100/numPerLine}}%' class="navItem">
      <image src="{{item.img}}" ></image>
      <view class="name">{{item.name}}</view>
    </view>
  </view>
</template>
